{
  "parser": "babel-eslint",
  "parserOptions": {
        "ecmaVersion": 8
  },
  "plugins": ["jest"],
  "rules": {
    "max-len": [1, 120, 2, { "ignoreComments": true }],
    "no-console": 0,
    "quotes": ["warn", "single"],
    "no-var": 1,
    "camelcase": ["error", {"properties": "always"}],
    "jsx-a11y/href-no-hash": [0]
  },
  "extends": ["eslint:recommended"],
  "env": {
    "node": true,
    "browser": true,
    "commonjs": true,
    "jquery": true,
    "jest/globals": true,
    "es6": true
  }
}
